    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "relay.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}